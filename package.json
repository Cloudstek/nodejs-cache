{
  "name": "@cloudstek/cache",
  "version": "1.0.0",
  "description": "Simple key/value (cache) store.",
  "author": "Maarten de Boer <maarten@cloudstek.nl>",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "dependencies": {
    "fs-extra": "^8.0.1",
    "moment": "^2.24.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^7.0.0",
    "@types/mockdate": "^2.0.0",
    "@types/node": "^12.0.2",
    "ava": "^1.4.1",
    "coveralls": "^3.0.3",
    "del-cli": "^1.1.0",
    "mockdate": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  },
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "clean": "npm-run-all -p clean:*",
    "clean:dist": "del-cli dist",
    "clean:test": "del-cli build",
    "clean:coverage": "del-cli coverage .nyc_output",
    "build": "tsc",
    "build:test": "tsc -p tsconfig.test.json",
    "build:ci": "npm-run-all -s clean build",
    "test": "npm-run-all clean:test clean:coverage build:test test:run clean:test",
    "test:run": "nyc ava",
    "test:ci": "npm-run-all -p clean:* -s build:test && nyc ava && nyc report --reporter=text-lcov | coveralls",
    "lint": "npm-run-all -l -p lint:src lint:test",
    "lint:src": "tslint src/**/*.ts",
    "lint:test": "tslint test/**/*.ts"
  },
  "ava": {
    "files": [
      "build/test/**/*"
    ]
  }
}
